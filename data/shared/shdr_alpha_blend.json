{
	"vertex_shader": "mesh.vert",
	"fragment_shader": "mesh.frag",
	"raster_state": {
		"cull_mode": "off",
		"depth_clip": true,
		"offset_factor": 0,
		"offset_units": 0,
		"conservative_raster": false
	},
	"depth_stencil_state": {
