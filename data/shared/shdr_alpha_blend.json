			{
				"blend_enable": true,
				"src_color_factor": "one",
				"dst_color_factor": "inv_src_alpha",
				"color_blend_op": "add",
				"src_alpha_factor": "one",
				"dst_alpha_factor": "inv_src_alpha",
				"alpha_blend_op": "add",
				"write_red": true,
				"write_green": true,
				"write_blue": true,
				"write_alpha": true
			}
		]
	}
}